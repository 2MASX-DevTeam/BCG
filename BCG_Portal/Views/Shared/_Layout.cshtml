@using Mvc.RazorTools.FontAwesome
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Mvc.RazorTools.BundleManager.Styles.Render()
    <link href="~/Content/custPortalCss.min.css" rel="stylesheet"/>


   
   
</head>
<body>


<div id="responsive-navbar">
    <ul>
        <li class="active">@Html.ActionLink("Home", "Index", "Home")</li>
        <li class='has-sub'>
            <a href='#'><span>Products</span></a>
            <ul>
                <li><a href='#'><span>Product 1</span></a></li>
                <li><a href='#'><span>Product 2</span></a></li>
                <li class='last'><a href='#'><span>Product 3</span></a></li>
            </ul>
        </li>
        <li class='has-sub'>
            @Html.ActionLink("About", "About", "Home")
            <ul>
                <li><a href='#'><span>Company</span></a></li>
                <li class='last'><a href='#'><span>Contact</span></a></li>
            </ul>
        </li>
        <li class="last">@Html.ActionLink("testt", "test", "Home")</li>
        <li class="last">@Html.ActionLink("Contact", "Contact", "Home")</li>
        @Html.Partial("_LoginPartial")
    </ul>
</div>
  

