<div class="container body-content">
    @RenderBody()
    <hr/>
    <footer>

        <div id="dateTime" class="custFooter">
            &copy; @DateTime.Now.Year
        </div>

        <div id="lastOne" class="custFooter"></div>

        <!--
        Html.FontAwesome(FontAwesomeIconSet.Star)

        Html.FontAwesome(FontAwesomeIconSet.FacebookSquare.Spin().Large2x())
        Html.FontAwesome(FontAwesomeIconSet.TwitterSquare.Large2x())
        Html.FontAwesome(FontAwesomeIconSet.Google.Large2x())
        Html.FontAwesome(FontAwesomeIconSet.Spinner, FontAwesomeStyles.Spin)
        -->



    </footer>
    <div id="socialIcons" class="custFooter">
        <!--facebook-->
        <span>
            <img class="custSocials" src="/images/facebook.ico" alt="" title="Facebook" />
        </span>

        <!--twitter-->
        <span>
            <img class="custSocials" src="/images/twitter.ico" alt="" title="Twitter" />
        </span>

        <!--google-->
        <span>
            <img class="custSocials" src="/images/google.ico" alt="" title="Google" />
        </span>

        <!--email-->
        <span>
            <img class="custSocials" src="/images/envelope.ico" alt="" title="Email" />
        </span>

        <!--skype-->
        <span>
            <img class="custSocials" src="/images/skype.ico" alt="" title="Skype" />
        </span>
    </div>
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
<script src="~/Scripts/custPortalScripts.js"></script>
@RenderSection("scripts", required: false)
</body>
</html>
